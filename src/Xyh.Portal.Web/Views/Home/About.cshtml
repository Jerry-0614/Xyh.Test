@using Xyh.Portal.Web.Startup
@{
    ViewBag.ActiveMenu = PageNames.About;
}

<h2>@L("About")!</h2>

<div class="row">
    <div class="col-lg-12">
        @L("About_Description")
    </div>
    <dl>
        @foreach (var claim in User.Claims)
        {
            <dt>@claim.Type</dt>
            <dd>@claim.Value</dd>
        }
    </dl>
</div>